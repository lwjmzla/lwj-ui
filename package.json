{
  "name": "bb-ui-vue3",
  "version": "0.1.0",
  "private": false,
  "cjs": "lib/bbui.cjs.js",
  "esm": "lib/bbui.esm.js",
  "main": "lib/index.js",
  "unpkg": "lib/bbui.unpkg.min.js",
  "scripts": {
    "package": "yarn install",
    "install:theme": "cd ./packages/theme-chalk/ && npm install && cd ../",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "build": "npm run rollup && npm run rollup:sub",
    "rollup": "rimraf outputFile/lib && rollup -c",
    "rollup:sub": "rimraf outputFile/lib-sub && node package-config/children.js && npm run rollup:theme",
    "rollup:theme": "rimraf lib-sub/theme-chalk && gulp build --gulpfile packages/theme-chalk/gulpfile.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@sufangyu/v-viewer": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.8",
    "axios": "^0.26.0",
    "bb-ui-theme-chalk": "file:packages/theme-chalk",
    "codemirror": "^5.64.0",
    "core-js": "^3.6.5",
    "dayjs": "^1.11.0",
    "lerna": "^4.0.0",
    "lodash-es": "^4.17.21",
    "number-precision": "^1.5.2",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "scss-to-css": "^0.1.0",
    "v-viewer": "^1.5.1",
    "vite-plugin-style-import": "^1.4.0",
    "vitepress": "0.22.3",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue": "3.2.20",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/plugin-transform-typescript": "^7.16.8",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "3.2.20",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "acorn-jsx": "^5.3.2",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.1.4",
    "element-plus": "2.1.8",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "gulp": "4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "patch-vue-directive-ssr": "^0.0.1",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.2.1",
    "rimraf": "^2.7.1",
    "rollup": "^2.60.2",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.2.10",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.45.2",
    "sass-loader": "^8.0.0",
    "typescript": "~4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "files": [
    "docs/.vitepress/dist",
    "lib",
    "lib-sub"
  ]
}
